{
  "name": "smartgrocer-cloud",
  "version": "1.0.0",
  "description": "Cloud-powered grocery management application",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "preview": "cd frontend && npm run preview",
    "backend:dev": "cd backend && uvicorn app.main:app --reload",
    "backend:test": "cd backend && pytest",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "install:all": "cd frontend && npm install && cd ../backend && pip install -r requirements.txt"
  },
  "keywords": [
    "grocery",
    "ecommerce",
    "react",
    "fastapi",
    "mongodb",
    "aws",
    "cloud"
  ],
  "author": "SmartGrocer Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2"
  }
}